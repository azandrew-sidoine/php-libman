{
    "name": "drewlabs/libman",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drewlabs\\Libman\\": "src/"
        },
        "files": [
            "./src/proxy.php"
        ]
    },
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "asmyns.platonnas29@gmail.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2",
        "symfony/process": "^6.1",
        "composer-runtime-api": "^2.0",
        "phpunit/phpunit": "^9.5",
        "drewlabs/crypt": "^0.2.3",
        "drewlabs/contracts": "^2.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Drewlabs\\Libman\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-contracts.git"
        }
    ],
    "suggest": {
        "ext-yaml": "YAML extension is required to use the YAML definiton loader"
    }
}
